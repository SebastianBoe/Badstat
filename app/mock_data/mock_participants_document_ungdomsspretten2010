function setItalic(elem)
{
	x=document.getElementById(elem);
	if (x != null)
		x.style.fontStyle="italic";
}
function resetItalic(elem)
{
	x=document.getElementById(elem);
	if (x != null)
		x.style.fontStyle="";
}
function tilbake()
{
    document.location='reg_login_form.php';
}
function tilbake_forenklet_paamelding(nbr)
{
    if (nbr == 6)
        document.location = 'reg_team_players.php';
    else
        document.location='reg_lag_onestep.php';
}
function setMousePos(ev)
{
   if(ev.pageX || ev.pageY)
   {
        x=ev.pageX;
        y=ev.pageY;
   }
   else
   {
       x=ev.clientX + document.body.scrollLeft - document.body.clientLeft;
       y=ev.clientY + document.body.scrollTop  - document.body.clientTop;
   }
   mTop = y;
   mLeft = x;
}

function vis_spillerkontaktinfo(lag_id, ev)
{
    setMousePos(ev);
    mp = document.getElementById('multipurpose');
    mp.style.top = mTop - 10;
    mp.style.left = mLeft + 20;
    //mp.style.height = 100;
    //mp.style.width = 250;

    var params = new Array();
    params['lag_id'] = lag_id
    straight_url_eval_params('vis_paamelding_spillerkontaktinfo.php', params);
}
function skjul_multipurpose()
{
    mp = document.getElementById('multipurpose')
    mp.innerHTML = '';
    mp.style.visibility='hidden';
}
//-->
